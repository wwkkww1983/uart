  <Port Datasize="1" Datatype="reg" Height="20" Function="" Side="right" Y="0.207627" Name="rx_buf_full" Width="20" Color="#b8860b" Parent="uart1" X="0.921875" Connect="" Inout="output"/>
  <Port Datasize="1" Datatype="reg" Height="20" Function="" Side="left" Y="0.177966" Name="tx" Width="20" Color="#7fff00" Parent="uart1" X="-0.078125" Connect="" Inout="output"/>
